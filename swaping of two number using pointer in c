#include <stdio.h>
void swap(int* x,int*y)
{
int temp;
temp=*x;
*x=*y;
*y=temp;
printf("%d",*x);
printf("%d",*y);
// return 0;
}
int main() {
    int n,b ;
    printf("enter the nmber");
    scanf("%d",&n);
    printf("enter the nmber");
    scanf("%d",&b);
    int*p=&n;
    int *d=&b;
    swap(&n,&b);
    return 0;
}
